{
  "name": "ts-bot",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:mock_config": "cp .env.example .env",
    "start": "run-s start:proxy start:stand",
    "prestart:proxy": "docker-compose --profile=dev-only up ngrok -d",
    "start:proxy": "./update_dev_public_url.sh",
    "poststart": "docker-compose down ngrok",
    "start:stand": "docker-compose up"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
